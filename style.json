{
    "QMainWindow": [
        {
            "title": "Data analysis",
            "icon": ":/All/icons_2/command.svg",
            "frameless": true,
            "transluscentBg": true,
            "navigation": [
                {
                    "minimize": "Btn_minimize",
                    "close": "Btn_close",
                    "restore": [
                        {
                            "buttonName": "Btn_maximize",
                            "normalIcon": ":/20x20/svg_icons/20x20/cil-window-maximize.svg",
                            "maximizedIcon": ":/20x20/svg_icons/20x20/cil-window-restore.svg"
                        }
                    ],
                    "moveWindow": "frame_header",
                    "tittleBar": "frame_header"
                }
            ]
        }
    ],
    "QStackedWidget": [
        {
            "name": "stackedWidget",
            "transitionAnimation": [
                {
                    "fade": [
                        {
                            "active": true,
                            "duration": 500,
                            "easingCurve": "OutBack"
                        }
                    ],
                    "slide": [
                        {
                            "active": true,
                            "duration": 500,
                            "direction": "horizontal",
                            "easingCurve": "OutBack"
                        }
                    ]
                }
            ],
            "navigation": [
                {
                    "navigationButtons": [
                        {
                            "Btn_1": "page_1",
                            "Btn_2": "page_2",
                            "Btn_3": "page_3",
                            "Btn_4": "page_4"
                        }
                    ]
                }
            ]
        }
    ],
    "QCustomSlideMenu": [
        {
            "name": "widget_leftmenu",
            "defaultSize": [
                {
                    "width": 0,
                    "height": "parent"
                }
            ],
            "collapsedSize": [
                {
                    "width": 0,
                    "height": "parent"
                }
            ],
            "expandedSize": [
                {
                    "width": 300,
                    "height": "parent"
                }
            ],
            "toggleButton": [
                {
                    "buttonName": "Btn_toggle",
                    "icons": [
                        {
                            "whenMenuIsCollapsed": ":/All/icons_2/align-justify.svg",
                            "whenMenuIsExpanded": ":/All/icons_2/align-left.svg"
                        }
                    ]
                }
            ],
            "menuTransitionAnimation": [
                {
                    "animationDuration": 500,
                    "animationEasingCurve": "OutBack"
                }
            ]
        }
    ]
}